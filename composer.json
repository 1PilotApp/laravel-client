{
    "name": "cmspilot/laravel-client",
    "description": "Client to connect a Laravel app to cmsPilot service",
    "keywords": ["laravel", "remote", "monitoring"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre-Andr√© Vullioud"
        }
    ],
    "require": {
        "composer/semver": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "CmsPilot\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CmsPilot\\Client\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CmsPilot\\Client\\ClientServiceProvider"
            ]
        }
    }
}
