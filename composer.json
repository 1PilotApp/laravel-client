{
    "name": "1pilotapp/laravel-client",
    "description": "Client to connect a Laravel app to 1Pilot.io service",
    "keywords": [
        "1Pilot",
        "laravel",
        "remote",
        "monitoring"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "1Pilot",
            "homepage": "https://1Pilot.io",
            "email": "support@1pilot.io"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-json": "*",
        "composer/semver": "^1.4",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/http": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/routing": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0|~3.8.0|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "OnePilot\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OnePilot\\Client\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "OnePilot\\Client\\ClientServiceProvider"
            ]
        }
    }
}
