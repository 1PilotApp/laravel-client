{
    "name": "1pilotapp/laravel-client",
    "description": "Client to connect a Laravel app to 1Pilot.io service",
    "keywords": [
        "1Pilot",
        "laravel",
        "remote",
        "monitoring"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre-Andr√© Vullioud"
        }
    ],
    "require": {
        "composer/semver": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "OnePilot\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OnePilot\\Client\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OnePilot\\Client\\ClientServiceProvider"
            ]
        }
    }
}
